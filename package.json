{
  "name": "autoiq-starter",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A setup for autoiq",
  "scripts": {
    "clean": "rm -rf node_modules doc dist && npm cache clean",
    "clean-start": "npm run clean && npm run clean && npm install && npm start",
    "watch": "webpack --watch --progress --profile",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "build": "rm -rf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "test-build": "npm run pretest && npm run test && rm -rf dist && npm run build",
    "lint": "tslint --force \"src/**/*.ts\"",
    "e2e": "protractor",
    "pretest": "npm run lint",
    "test": "karma start",
    "test-start": "npm run test && npm run start",
    "complete-test": "npm run e2e && npm run test",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "clean-test-start": "npm run clean && npm install && npm run test-start",
    "postinstall": "typings install",
    "jserver":"json-server db.json",
    "mock": "npm run jserver | npm start"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.0",
    "express": "^4.13.3",
    "opensans-npm-webfont": "0.0.2",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "autoprefixer": "^6.3.2",
    "codelyzer": "0.0.26",
    "copy-webpack-plugin": "^3.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.4.0",
    "jquery": "^3.1.0",
    "json-loader": "^0.5.3",
    "json-server": "^0.8.17",
    "karma": "^0.13.22",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^3.1.1",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "bootstrap-loader": "^1.1.0",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.23.1",
    "postcss-loader": "^0.9.1",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.2",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.1",
    "tslint": "^3.4.0",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  }
}
